@{
ViewBag.Title = "Index";
}

<h1>@ViewData["LabNum"]</h1>
<h2>Список</h2>
<ol type="A">
    @for (int i = 1; i <= 5; i++)
    {
        <li>Элемент @i списка</li>
    }
</ol>
<hr />
<h2>Форма</h2>
<form method="post">
    <div class="row mb-3">
        <select id="itemSelect" name="selectedItemId" class="form-control" asp-items="ViewBag.ItemList"></select>
    </div>
    <div class="row mb-3">
        <div class="col-sm-10 offset-sm-2">
                       
                <input type="checkbox" name="check-box1" />
                <label for="check-box1"> Чек-бокс 1</label>
                <br />
                <input type="checkbox" name="check-box2" />
                <label for="check-box2"> Чек-бокс 2</label>
        </div>
    </div>
              
    <fieldset class="row mb-3">
        <div class="col-sm-10 offset-sm-2">
            <input type="radio" name="radio" id="radio1" value="1" />
            <label for="radio1">Радио-1</label>
            <input type="radio" name="radio" id="radio2" value="2" />
            <label for="radio2">Радио-2</label>
            <input type="radio" name="radio" id="radio3" value="3" />
            <label for="radio3">Радио-3</label>
        </div>
    </fieldset>
    <div class="row mb-3">
        <label for="inputLogin3" class="col-sm-2 col-form-label">Логин</label>
        <div class="col-sm-10">
            <input class="form-control" id="inputLogin3" name="login">
        </div>
    </div>
    <div class="row mb-3">
        <label for="inputPassword3" class="col-sm-2 col-form-label">Пароль</label>
        <div class="col-sm-10">
            <input type="password" class="form-control" id="inputPassword3" name="password">
        </div>
    </div>
    <button type="submit" class="btn btn-primary">Отправить</button>
</form>